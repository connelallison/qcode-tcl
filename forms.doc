doc forms {
    Title "Qcode Forms"
    Description {
	<h3>Widgets</h3>
	<ul>
	<li>Form Label [doc_link widget_label]</li>
	<li>Text Input Box [doc_link widget_text]</li>
	<li>Combo Input Widget [doc_link widget_combo]</li>
	<li>HTML Editor Control [doc_link widget_htmlarea]</li>
	<li>Textarea Input [doc_link widget_textarea]</li>
	<li>Compare Input Control [doc_link widget_compare]</li>
	<li>Drop Down List [doc_link widget_select]</li>
	<li>Checkbox Control [doc_link widget_checkbox]</li>
	<li>Boolean Checkbox [doc_link widget_bool]</li>
	<li>Password Text Input [doc_link widget_password]</li>
	<li>Submit Button [doc_link widget_submit]</li>
	<li>Button [doc_link widget_button]</li>
	<li>Radio Button [doc_link widget_radio]</li>
	<li>Radio Button Group [doc_link widget_radiogroup]</li>
	<li>Span [doc_link widget_span]</li>
	</ul>
	<h3>Form Layout</h3>
	<ul>
	<li>[doc_link form_layout_tbody]</li>
	<li>[doc_link form_layout_table]</li>
	<li>[doc_link form_layout_tables]</li>
	<li>[doc_link form_layout_list]</li>
	</ul>
    }

}